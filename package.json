{
  "name": "ali-ocs",
  "version": "1.0.0",
  "description": "Aliyun OCS(open cache storage) client.",
  "main": "index.js",
  "files": [
    "lib", "index.js"
  ],
  "scripts": {
    "test": "mocha --check-leaks -t 30000 -r thunk-mocha test/*.test.js",
    "test-cov": "istanbul cover node_modules/.bin/_mocha -- -R dot --check-leaks -t 60000 -r thunk-mocha test/*.test.js",
    "jshint": "jshint .",
    "autod": "autod -w --prefix '~' --devprefix '^' -D mocha,autod,should,thunk-mocha,istanbul"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ali-sdk/ali-ocs.git"
  },
  "keywords": [
    "memcached",
    "ocs",
    "client",
    "aliyun"
  ],
  "author": "fool2fish <fool2fish@gmail.com> (https://github.com/fool2fish)",
  "license": "MIT",
  "homepage": "https://github.com/ali-sdk/ali-ocs",
  "bugs": {
    "url": "https://github.com/ali-sdk/ali-ocs/issues"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "dependencies": {
    "debug": "~2.2.0",
    "long": "~3.0.3",
    "is": "~3.1.0",
    "java.io": "~2.1.1"
  },
  "devDependencies": {
    "autod": "^2.4.1",
    "istanbul": "^0.4.1",
    "mocha": "^2.3.4",
    "expect.js": "~0.3.1",
    "thunk-mocha": "^1.0.1",
    "pedding": "*"
  }
}
